{% extends "template.html.twig" %}

{% block body %}
    <table class="table">
    {% for note in notes %}
    <tr></tr>
    <td>{{note}}</td>
    
    
    {% endfor %}
    </table>
    <div class="alet alert-info">
    <p><i class="fa fa-bell" aria-hidden="true"></i> nombre de notes: {{notes | length}}</p>
    👍 🇦🇱 💯
    <p> te max: {{max(notes)}}</p>
    <p> note min: {{min(notes)}}</p>


    </div>
{% endblock %}